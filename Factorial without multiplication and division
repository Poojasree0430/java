class Main{
    public static void main(String[] args){
        int result=1;
        int n=5;
        for(int i=1;i<=n;i++){
            int sum=0;
            for(int j=1;j<=i;j++){
                sum=sum+result;
            }
            result=sum;
        }
        System.out.println(result);
    }
}
