class Main{
    public static void main(String[] args){
        int count=0;
        String s1="abacabadabacaba";
        String s2="abcd";
        for(int i=0;i<s1.length();i++){
            for(int j=0;j<s2.length();j++){
                if(s2.charAt(j)==s1.charAt(i)){
                    count++;
                }
            }
        }
        System.out.println(count);

    }
}
