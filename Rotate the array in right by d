import java.util.*;
class Main{
    public static void main(String[] args){
        int a[]={1,2,3,4,5};
        int d=2;
        int n=a.length;
        for(int i=0;i<d;i++){
            int temp=a[n-1];
            for(int j=n-1;j>0;j--) {
                a[j] = a[j-1];
            }
            a[0]=temp;}
        System.out.println(Arrays.toString(a));
            }
        }
