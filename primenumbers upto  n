
//----------primenumbers upto  n------

class Main{
    public static void main(String[] args){
for(int i=0;i<=10;i++){
    if(isprime(i)){
        System.out.println(i);
    }
}

    }
    public static boolean isprime(int n){
        if(n<2) return false;
        for(int i=2;i<=n/2;i++){
            if(n%i==0) {
                return false;
            }
        }

        return true;
    }
}
