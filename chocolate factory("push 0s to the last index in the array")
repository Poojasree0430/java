import java.util.*;
class Main{
    public static void main(String[]args){
        int a[]={2,3,0,7,0,4};
        int j=0;
        for(int i=0;i<a.length;i++){
            if(a[i]!=0){
                a[j]=a[i];
                j++;
            }

            }
        while(j>a.length){
            a[j]=0;
            j++;
        }
        System.out.println(Arrays.toString(a));
        }
    }
