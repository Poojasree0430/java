import java.util.*;
class Main{
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("enter n");
        int n=sc.nextInt();
        System.out.println("Enter the digits:");
        int a[] = new int[n];
        for (int i = 0; i < n; i++) {
          a[i]= sc.nextInt();
        }for(int i=0;i<n;i++) {
            char ch=(char)a[i];
            System.out.println(a[i]+"-"+ch);
        }
    }
}
