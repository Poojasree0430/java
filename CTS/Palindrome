import java.util.*;
class Main{
    public static void main(String[] args){
        Scanner scn=new Scanner(System.in);
       int n=scn.nextInt();
        int org=n;
        int rem,rev=0;
        while(n>0){

            rem=n%10;
            rev=rev*10+rem;
            n=n/10;
        } if(rev==org){
            System.out.println("Palindrome");
        }else{
            System.out.println("Not a Palindrome");
        }
    }
}
