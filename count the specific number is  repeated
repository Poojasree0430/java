 import java.util.*;
	public class Main {
	    public static void main(String[] args) {
	         int[]a={2,5,5,3,7,3};
	                int n=a.length;
	                int target=5;
	                int count=0;
	                for(int i=0;i<n;i++){
	           
	              if(a[i]==target){
	           count++;}}
	           System.out.print(count);
    }} 
