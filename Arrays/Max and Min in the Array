import java.util.*;
class Main{
    public static void main (String[] args){
     int a[]={3,4,8,5,6,2};
     int n=a.length;
     int max=0;
      int min=a[0];
     for(int i=0;i<n;i++){
         if(a[i]>max){
             max=a[i];
         }
        if(a[i]<min){
            min=a[i];
        }

     }
       System.out.println("Min:"+min);
        System.out.println("Max:"+max);

    }
}
