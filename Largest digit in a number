
//---------Largest digit in a number-----

class Main{
    public static void main(String[] args){
        int n=12345,rem;
        int largest=0;
        while(n!=0){
           rem=n%10;
           if(rem>largest){
               largest=rem;
           }
           n=n/10;
        }
        System.out.print(largest);

    }
}
