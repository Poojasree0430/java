public class JavaApplication9{
    public static  void main(String[] args){
        int a[][]={{5,4,3,6},{1,2,3,6},{12,7,16,18},{2,3,5,8}};
        int n=a.length;
        int count=0,j=0;
        for(int i=0;i<n;i++){
         
        for( j=0;j<a[0].length-1;j++){
            if(a[i][j+1]<=a[i][j]){
              
              break;
              }}
         if(j==a[0].length-1){
            count++;
        }
        
    }
       System.out.print(count); 
    }
}
