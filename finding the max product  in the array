public class JavaApplication9 {

   
    public static void main(String[] args) {
       int a[]={2,4,5,1,6,7};
       int n=a.length;
       int pro=1;
       for(int i=0;i<n-1;i++){
           int total=a[i]*a[i+1];
           if(total>pro){
               pro=total; }
       }System.out.print(pro+" ");
}}
