class Main{
    public static void main(String[] args){
        String a="Hello";
      int v=0,c=0;
        for(int i=0;i<a.length();i++){
            char ch=a.charAt(i);
            if("aeiouAEIOU".indexOf(ch)!=-1){
                v++;
            }else{
                c++;
            }
           
        }System.out.println("vowels="+v);
        System.out.println("consonant="+c);
    }
}
